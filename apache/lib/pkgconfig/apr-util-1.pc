prefix=/usr/local/lamp_dev_64/apache
exec_prefix=/usr/local/lamp_dev_64/apache
libdir=${prefix}/lib
APRUTIL_MAJOR_VERSION=1
includedir=/usr/local/lamp_dev_64/apache/include

Name: APR Utils
Description: Companion library for APR
Version: 1.3.11
# assume that apr-util requires libapr of same major version
Requires: apr-1
Libs: -L${libdir} -laprutil-${APRUTIL_MAJOR_VERSION}  -lexpat
Cflags: -I${includedir}

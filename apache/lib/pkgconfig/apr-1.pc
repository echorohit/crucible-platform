prefix=/usr/local/lamp_dev_64/apache
exec_prefix=/usr/local/lamp_dev_64/apache
libdir=${prefix}/lib
APR_MAJOR_VERSION=1
includedir=/usr/local/lamp_dev_64/apache/include

Name: APR
Description: The Apache Portable Runtime library
Version: 1.4.4
Libs: -L${libdir} -lapr-${APR_MAJOR_VERSION} -lrt -lcrypt  -lpthread -ldl
Cflags: -D_REENTRANT -D_GNU_SOURCE -g -O2 -pthread -I${includedir}
